
@{
    ViewData["Title"] = "Secrate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Secrate</h1>
@if(User.Identity.IsAuthenticated)
{
    <table class="table table-sm">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
        
    </table>
}
